{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
    }
    },
    "extends": [
      "eslint:recommended",
      "prettier",      
      "plugin:react/recommended"
    ],
    "env": {
      "es6": true,
      "node": true,
      "mocha": true,
      "browser": true
    },
    "rules": {
      "no-console": "warn"
    }
  }
  